mixin list(items)
  ul(class!=attributes.class)
    each item, key in items
      li(class=active)
        +link(item.title, item.href, item.text)(class=item.class)

mixin listImg(items)
  ul(class!=attributes.class)
    each item, key in items
      li(class=active)
        +linkImg(item.title, item.href, item.text, item.path)(class=item.class)
